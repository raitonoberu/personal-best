package view

templ Login() {
	@index("Вход") {
		<h1>Вход</h1>
		<form action="/login" method="post">
			<div>
				<label for="email">Почта</label>
				<input type="text" name="email" id="email"/>
			</div>
			<div>
				<label for="password">Пароль</label>
				<input type="password" name="password" id="password"/>
			</div>
			<button type="submit">Войти</button>
		</form>
	}
}
